{
  "compilerOptions": {
    "target": "es2017",
    "diagnostics": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "sourceMap": false,
    "moduleResolution": "classic",
    "stripInternal": true,
    "pretty": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noUnusedLocals": true,
    "strict": true,
    "strictNullChecks": true,
    "lib": [
      "es2017"
    ],
    "types": [],
    "outFile": "dist/amd.js",
    "baseUrl": "."
  },
  "include": ["src/amd.ts"],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
